                                                </td>
                                                <td width="30%" style='text-align:center;'>
                                                    <span class="text"><strong>Representative</strong></span>
                                                </td>
                                            </tr>
                                        </table>
                                    </li>
                                    @if (Model.TotalEnquiryList.Count() > 0)
                                    {
                                        foreach (var item in Model.TotalEnquiryList)
                                        {
                                            <li>
                                                <table style="width:100%">
                                                    <tr>
                                                        <td width="10%">
                                                            <span class="text"><a href="javascript:location.href = '/Enquiry/#edit/@item.Id'">@item.Id</a></span>
                                                        </td>
                                                        <td width="30%" style='text-align:center;'>
                                                            <span class="text">@item.Date.Value.ToString("dd-MMM-yyyy")</span>
                                                        </td>
                                                        <td width="20%">
                                                            <span class="text pull-right">
                                                                @if (item.Total.HasValue == true)
                                                                {
                                                                    @item.Total.Value.ToString("#,##0.00")
                                                                }
                                                                else
                                                                {
                                                                    @str;
                                                                }
                                                            </span>
                                                        </td>
                                                        <td width="30%">
                                                            <span class="text pull-right">@item.AssignedUsername</span>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </li>
                                        }
                                    }

