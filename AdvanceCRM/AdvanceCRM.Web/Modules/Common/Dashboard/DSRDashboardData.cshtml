<div class="box box-primary collapsed-box">
    <div class="box-header">
        <i class="fa fa-handshake-o"></i>
        <h3 class="box-title">Followups</h3>
        <button class="btn btn-primary btn-sm pull-right" data-widget="collapse" data-toggle="tooltip" title="Collapse" style="margin-right: 5px;"><i class="fa fa-plus"></i></button>
    </div><!-- /.box-header -->
    <div class="box-body">
        @if (Model.EnqFollowups.Count > 0)
        {
            <div class="col-md-12">
                <div class="box box-solid">
                    <div class="box-header with-border bg-aqua">
                        <h3 class="box-title">Enquiry (@Model.EnqFollowups.Count)</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-footer no-padding">
                        <div class="table-responsive">
                            <table class="table no-margin">
                                <thead>
                                    <tr>
                                        <th>Followup Note</th>
                                        <th>Contact</th>
                                        <th>Details</th>
                                        <th>Status</th>
                                        <th>Date</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.EnqFollowups)
                                    {
                                        <tr>
                                            <td>@item.FollowupNote</td>
                                            <td>@item.ContactName</td>
                                            <td>@item.Details</td>
                                            <td>@item.Status</td>
                                            <td>@item.FollowupDate.ToDateFormat()</td>
