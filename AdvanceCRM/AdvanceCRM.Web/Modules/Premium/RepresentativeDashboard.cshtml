				//ConversionRatioEnquiry Angular Gauge

				$.ajax({
					url: '@Url.Action("RepresentativeEnquiryRatioChart", "RepresentativeDashboard")?StartDate=' +
					'@System.DateTime.Now.AddMonths(-1).ToString("MM/dd/yyyy")' + '&EndDate=' + '@System.DateTime.Now.ToString("MM/dd/yyyy")' + '&ListId=' + this.value,

					cache: false,
					type: "GET",
