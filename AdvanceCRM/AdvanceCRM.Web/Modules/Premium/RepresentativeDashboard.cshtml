					success: function (data) {

						if (JSON.parse(data).length == 0) {
							$("#lblMsgConversionRatioEnquiry").show();
							$("#MsgConversionRatioEnquiry").text("Data is not available for last 30 days,kindly select Date range");
						}

						var gaugeChart = AmCharts.makeChart("ConversionRatioEnq", {
							"type": "gauge",
							"responsive": {
								"enabled": true
							},
							"theme": "light",
							"axes": [{
								"axisThickness": 1,
								"axisAlpha": 0.2,
								"tickAlpha": 0.2,
								"valueInterval": 5,
								"bands": [{
									"color": "#cc4748",
									"endValue": 30,
									"innerRadius": "70%",
									"startValue": 0
								}, {
									"color": "#fdd400",
									"endValue": 70,
									"innerRadius": "70%",
									"startValue": 30
								}, {
									"color": "#84b761",
									"endValue": 100,
									"innerRadius": "70%",
									"startValue": 70
								}],
								"bottomText": "0 Conversion",
								"bottomTextYOffset": -20,
								"endValue": 100
							}],
							"arrows": [{}],
							"export": {
								"enabled": true
							}
						});

						setInterval(randomValueEnqInitial(data), 2000);
						// set random value
						function randomValueEnqInitial(Enqdata) {
							var value = Enqdata;
							if (gaugeChart) {
								if (gaugeChart.arrows) {
									if (gaugeChart.arrows[0]) {
										if (gaugeChart.arrows[0].setValue) {
											gaugeChart.arrows[0].setValue(value);
											gaugeChart.axes[0].setBottomText("Enquiry Conversion" + "   " + (value + ("%")));
										}
									}
								}
							}
						}

