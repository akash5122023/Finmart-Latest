			 //Representative Sales status  pyramid
				$.ajax({
					url: '@Url.Action("SalesStatusChart", "RepresentativeDashboard")?StartDate=' +
					'@System.DateTime.Now.AddMonths(-1).ToString("MM/dd/yyyy")' + '&EndDate=' + '@System.DateTime.Now.ToString("MM/dd/yyyy")' + '&ListId=' + this.value,

