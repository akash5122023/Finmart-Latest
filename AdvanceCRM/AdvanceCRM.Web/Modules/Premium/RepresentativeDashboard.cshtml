						cache: false,
						type: "GET",
						dataType: "html",
						success: function (data) {
							//alert(JSON.parse(data).length);
							if (JSON.parse(data).length == 0) {
								$("#lblMsgEnquiryStages").show();
								$("#MsgEnquiryStages").text("Data is not available for last 30 days,kindly select Date range");
							}
							if (JSON.parse(data).length != 0) {
								$("#lblMsgEnquiryStages").hide();
								$("#MsgEnquiryStages").text("");
							}

							//RepresentativeEnquiryStages Funnel
							var chart = AmCharts.makeChart("RepresentativeEnquiryStages", {
								"type": "funnel",
								"theme": "light",
								"responsive": {
									"enabled": true
								},
								"dataProvider": JSON.parse(data),
								"balloon": {
									"fixedPosition": true
								},
								"valueField": "value",
								"titleField": "title",
								"labelText": "[[title]]([[value]], Amt:[[sum]])",
								"marginRight": 240,
								"marginLeft": 50,
								"startX": -500,
								"depth3D": 100,
								"angle": 40,
								"outlineAlpha": 1,
								"outlineColor": "#FFFFFF",
								"outlineThickness": 2,
								"labelPosition": "right",
								"balloonText": "[[title]](Count:[[value]], Amt:[[sum]])",
