
@model IEnumerable<MailChimp.Net.Models.List>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-striped table-bordered ">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Created On</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var list in Model)
        {
        <tr>
            <td>@list.Id</td>
            <td>@list.Name</td>
            <td>@list.DateCreated</td>
        </tr>
        }
    </tbody>
</table>


