@inject Serenity.ITextLocalizer Localizer
@{
     ViewData["Title"] = Localizer.Get("Db.Settings.BizMailConfig.EntityPlural");
}

@section ContentHeader {
    <h1>@ViewData["Title"]</h1>
}

<div id="GridDiv"></div>

<script type="text/javascript">
    jQuery(function () {
        new AdvanceCRM.Settings.BizMailConfigDialog().loadByIdAndOpenDialog(1);
        Q.initFullHeightGridPage($('#GridDiv'));
    });
</script>