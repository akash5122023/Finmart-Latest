{
  
  "Data": {
    "Default": {
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=AdvanceCRM_Default_Finmart_v1;Trusted_Connection=true;TrustServerCertificate=true;Connection Timeout=60;",
      "ProviderName": "System.Data.SqlClient"
    },
    "Log": {
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=AdvanceCRM_Default_FinMart_v1;Trusted_Connection=true;TrustServerCertificate=true;Connection Timeout=60;",
      "ProviderName": "System.Data.SqlClient"
    }
  },

  "SmtpSettings": {
    "Host": "smtp.hostinger.com",
    "Port": 465,
    "UseSsl": true,
    "From": "connect@bizonapp.com",
    "Username": "connect@bizonapp.com",
    "Password": "Connectbizon@123"
  },
  // The following sections are only required for the multi-tenant SaaS deployment.
  // For a single-tenant / single-customer deployment they are intentionally kept
  // in a disabled / no-op state.
  "Cloudflare": {
    // API token can also be provided via the CLOUDFLARE_API_TOKEN environment variable
    "ApiToken": "",
    // Zone identifier or CLOUDFLARE_ZONE_ID environment variable
    "ZoneId": "",
    // Server IP or CLOUDFLARE_SERVER_IP environment variable
    "ServerIp": "",
    // Root domain or CLOUDFLARE_ROOT_DOMAIN environment variable
    "RootDomain": ""
  },
  "MultiTenancy": {
    // Empty root domain and disabled enforcement ensure that all requests are
    // treated as a single host context instead of per-tenant subdomains.
    "RootDomain": "",
    "DefaultConnectionKey": "Default",
    "ConnectionKeys": [
      "Default",
      "Log"
    ],
    "EnforceTenant": false,
    "MarketingHosts": [],
    "StaticTenants": { }
  },

  // Trial and plan pricing settings are kept for compatibility with existing
  // code, but they no longer affect core CRM usage in single-tenant mode.
  "TrialSettings": {
    "DefaultDays": 0,
    "DefaultUserLimit": 0,
    "Plans": { },
    "UserLimits": { }
  },
  "PlanPricing": {
    "CurrencySymbol": "\u20B9",
    "CurrencyCode": "INR",
    "Plans": { }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://seq:5341" }
      },
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "AdvanceCRM"
    }
  },
  "CssBundling": {
    "Enabled": false,
    "Minimize": true,
    "UseMinCss": true
  },
  "LocalTextPackages": {
    "Site": "^(Controls|Db|Dialogs|Enums|Forms|Permission|Site|Validation)\\.",
    "Login": "^(Forms\\.Membership\\.Login|Db\\.Administration\\.User|Validation\\.Required|Dialogs)\\."
  },
  "ScriptBundling": {
    "Enabled": false,
    "Minimize": true,
    "UseMinJS": true
  },
  "UploadSettings": {
    "Path": "~/App_Data/upload/"
  },
  "ExpiryDate": {
    "Date": "31/10/2023"
  }
}


